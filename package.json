{
  "name": "shin-code-editor",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "Shin Code Editor",
  "author": "Shin Tachibana <thanhnguyennguyen1995@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "dev": "quasar dev",
    "setup": "./setup",
    "dev:android": "quasar dev -m capacitor -T android",
    "dev:ios": "quasar dev -m capacitor -T ios",
    "build:android": "quasar build -m capacitor -T android",
    "build:ios": "quasar build -m capacitor -T ios",
    "build:templates": "node resources/build.js",
    "open:android": "cd src-capacitor && cap open android && cd ..",
    "run:android": "yarn build:templates && yarn build:android && cd src-capacitor && cap sync android && cap open android && cd .."
  },
  "dependencies": {
    "@capacitor/android": "^3.2.0",
